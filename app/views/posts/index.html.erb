<center><h1>Latest News From Your Network</h1></center><Br><br>


<% if @all_posts.count == 0 %>
  <h3>You don't have any posts to view! Get out there and <%= link_to "follow", users_path %> some more users!<h3>
<% else %>
  <% @all_posts.each do |post| %>
    <%= render partial: 'thumbnail', locals: {post: post} %>
  <% end %>

    <center><%= will_paginate @all_posts %></center>

<% end %>
